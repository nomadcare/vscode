{
  "name": "vibe-coding",
  "displayName": "Vibe Coding",
  "publisher": "your-publisher",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.60.0"
  },
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vibeCoding",
          "title": "Vibe",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "vibeCoding": [
        {
          "type": "webview",
          "id": "vibeCodingView",
          "name": "Vibe Coding"
        }
      ]
    }
  },
  "scripts": {
    "compile": "tsc -p ./",
    "build-webview": "node esbuild.config.js",
    "watch-webview": "node esbuild.config.js --watch",
    "watch-extension": "tsc -w -p ./",
    "build": "npm run compile && npm run build-webview",
    "watch": "concurrently \"npm run watch-extension\" \"npm run watch-webview\""
  },
  "devDependencies": {
    "@types/node": "^14.18.63",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@types/vscode": "^1.100.0",
    "@vscode/test-electron": "^2.3.0",
    "concurrently": "^9.1.2",
    "esbuild": "^0.25.4",
    "typescript": "^4.9.5",
    "vscode": "^1.1.37"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  }
}
